<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <attr name="rightPadding" format="dimension"/>

    <declare-styleable name="SlidingMenu">
        <attr name="rightPadding"/>
    </declare-styleable>

    <declare-styleable name="PublicView">
        <attr name="left_logo" format="reference"/>
        <attr name="hint" format="string"/>
        <attr name="right_peroson" format="boolean"/>
        <attr name="right_code" format="boolean"/>
        <attr name="text" format="string"/>
    </declare-styleable>

    <declare-styleable name="ImageTextImageView">
        <attr name="left_logo_iti" format="reference"/>
        <attr name="right_go_on" format="reference"/>
        <attr name="text_iti" format="string"/>
    </declare-styleable>
    <declare-styleable name="TextTextImageView">
        <attr name="tti_right_go_on" format="reference"/>
        <attr name="tti_text" format="string"/>
        <attr name="tti_text_color" format="color"/>
        <attr name="tti_title" format="string"/>
    </declare-styleable>

    <declare-styleable name="RatingBar" tools:ignore="ResourceName">
        <!--尺寸值-->
        <attr name="starImageSize" format="dimension"/>
        <attr name="starCount" format="integer"/>
        <!--资源文件值-->
        <attr name="starEmpty" format="reference"/>
        <!--资源文件值-->
        <attr name="starFill" format="reference"/>
        <!--资源文件值-->
        <attr name="starHalf" format="reference"/>
        <!--boolean值-->
        <attr name="clickable" format="boolean"/>
    </declare-styleable>
</resources>