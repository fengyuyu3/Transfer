<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/id_cord_background"
    android:orientation="vertical">

    <include
        layout="@layout/include_toolbar"
        ></include>

    <com.ironaviation.traveller.mvp.ui.widget.TextTextImageView
        android:id="@+id/tti_identification"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="32px"
        app:tti_right_go_on="@mipmap/ic_gray_right"
        app:tti_title="@string/setting_identification"
        ></com.ironaviation.traveller.mvp.ui.widget.TextTextImageView>
    <include layout="@layout/include_line_one"/>
    <com.ironaviation.traveller.mvp.ui.widget.TextTextImageView
        android:id="@+id/tti_usual_address"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:tti_right_go_on="@mipmap/ic_gray_right"
        app:tti_title="@string/usual_address"
        ></com.ironaviation.traveller.mvp.ui.widget.TextTextImageView>
    <include layout="@layout/include_line_one"/>
    <com.ironaviation.traveller.mvp.ui.widget.TextTextImageView
        android:id="@+id/tti_connect_us"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:tti_right_go_on="@mipmap/ic_gray_right"
        app:tti_text="@string/us_phone"
        app:tti_title="@string/connect_us"
        ></com.ironaviation.traveller.mvp.ui.widget.TextTextImageView>
    <include layout="@layout/include_line_one"/>
    <com.ironaviation.traveller.mvp.ui.widget.TextTextImageView
        android:id="@+id/tti_about_us"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:tti_title="@string/about_us"
        app:tti_right_go_on="@mipmap/ic_gray_right"
        ></com.ironaviation.traveller.mvp.ui.widget.TextTextImageView>

    <!--<com.ironaviation.traveller.mvp.ui.widget.TextTextImageView
        android:id="@+id/tti_cancellation_account"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="32px"
        app:tti_title="@string/cancellation_account"
        ></com.ironaviation.traveller.mvp.ui.widget.TextTextImageView>-->

    <TextView
        android:background="@drawable/select_btn_grey"
        android:textSize="30px"
        android:id="@+id/tti_cancellation_account"
        android:textColor="@color/red_deep"
        android:gravity="center"
        android:layout_gravity="center"
        android:layout_marginTop="32px"
        android:text="@string/cancellation_account"
        android:layout_width="match_parent"
        android:layout_height="112px"/>

   <!-- <TextView
        android:id="@+id/tv_version_name"
        style="@style/common_text_content_style"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="60px"
        android:gravity="bottom"

        android:textColor="@color/input_box_grey_un_select"
        />-->
</com.zhy.autolayout.AutoLinearLayout>
